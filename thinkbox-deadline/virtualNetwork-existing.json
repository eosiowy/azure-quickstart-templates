{
  	"$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  	"contentVersion": "1.0.0.0",
  	"parameters": {
	    "location": {
	      "type": "string"
	    },
	    "virtualNetworkName": {
	    	"type": "string"
    	},
	    "virtualNetworkAddressPrefix": {
	    	"type": "string"
		},
	    "subnetName": {
	    	"type": "string"
	    },
	    "subnetAddressSpace": {
	    	"type": "string"
		},
	    "networkSecurityGroupName": {
	    	"type": "string"
		},
		"existingVirtualNetworkRG": {
			"type": "string",
			"defaultValue": ""
		}
	},
	"resources": [],
	"outputs": {
		"virtualNetworkId": {
			"value": "[resourceId(parameters('existingVirtualNetworkRG'), 'Microsoft.Network/virtualNetworks', parameters('virtualNetworkName') )]",
			"type": "string"
		},
		"subnetId": {
			"value": "[resourceId( parameters('existingVirtualNetworkRG'), 'Microsoft.Network/virtualNetworks', parameters('virtualNetworkName'), '/subnets/', parameters('subnetName') )]",
			"type": "string"
		}
	}
}