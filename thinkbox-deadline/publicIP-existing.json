{
  	"$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  	"contentVersion": "1.0.0.0",
  	"parameters": {
	    "location": {
	      "type": "string"
	    },
	    "publicIPName": {
	    	"type": "string"
    	},
	    "dnsName": {
	    	"type": "string"
		},
		"existingPublicIPRG": {
			"type": "string",
			"defaultValue": ""
		}
	},
	"resources": [],
	"outputs": {
		"publicIPId": {
			"value": "[resourceId(parameters('existingPublicIPRG'), 'Microsoft.Network/publicIPAddresses', parameters('publicIPName') )]",
			"type": "string"
		}
	}
}